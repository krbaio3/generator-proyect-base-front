  "scripts": {
    "lint": "tslint src/**/*.ts",
    "start": "node build/dev-server.js",
    "start:prod": "NODE_ENV=production node build/dev-server.js",
    "build":
      "npm run clean && NODE_ENV=production webpack --config build/webpack.prod.conf.js --progress --profile",
    "server": "node node node_modules/.bin/http-server dist --cors",
    "build:dev":
      "npm run clean && webpack --config build/webpack.dev.conf.js --progress --profile",
    "clean": "node node_modules/.bin/rimraf dist",
    "debug": "node --nolazy --inspect-brk=9229 build/dev-server.js",
    "test": "node node_modules/.bin/karma start test/karma.conf.js",
    "protractor": "node node_modules/.bin/protractor e2e/protractor.conf.js",
    "preprotractor":
      "node node_modules/typescript/bin/tsc --project e2e/tsconfig.e2e.json && node node_modules/protractor/bin/webdriver-manager update"
  },
